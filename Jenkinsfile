pipeline {
  agent any
  stages {
    stage('STEP 1') {
      steps {
        echo 'Hi'
      }
    }
    stage('Step 2 ') {
      steps {
        echo 'Hello world step 2 '
      }
    }
    stage('Step 3 ') {
      steps {
        echo 'Step 3 '
      }
    }
    stage('step 4') {
      steps {
        echo 'hello from the other fucking side'
      }
    }
    stage('step 5') {
      steps {
        echo 'slim shady'
      }
    }
  }
}